{
  "name": "quiz-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "client-install": "cd client && env-cmd -f src/config/config.env npm install",
    "client-build": "cd client && env-cmd -f src/config/config.env npm run build",
    "start": "env-cmd -f src/config/config.env node index",
    "server": "env-cmd -f src/config/config.env nodemon index",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "mongoose-auto-increment": "^5.0.1",
    "nodemailer": "^6.4.5",
    "nodemon": "^2.0.2",
    "react-fullscreen-loading": "0.0.4",
    "react-loading-overlay": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "env-cmd": "^10.1.0"
  }
}
